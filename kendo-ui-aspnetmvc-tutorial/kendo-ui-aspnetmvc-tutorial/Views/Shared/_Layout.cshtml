<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>@ViewBag.Title - UI for MVC / Kendo Quick Start Guide</title>

  <link href="https://cdn.kendostatic.com/2022.3.1109/styles/kendo.common.min.css" rel="stylesheet" type="text/css" />
  <link href="https://cdn.kendostatic.com/2022.3.1109/styles/kendo.default.min.css" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
  <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
  <script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
  <script src="https://unpkg.com/jszip/dist/jszip.min.js"></script>
  <script src="https://cdn.kendostatic.com/2022.3.1109/js/kendo.all.min.js"></script>
  <script src="https://cdn.kendostatic.com/2022.3.1109/js/kendo.aspnetmvc.min.js"></script>

  @Styles.Render("~/Content/css")
  <link href="~/Content/site-nova.css" rel="stylesheet" />
</head>
<body>
  <div class="container-fluid body-content">
    @RenderBody()
  </div>

  @Scripts.Render("~/bundles/bootstrap")
  @RenderSection("scripts", required: false)
</body>
</html>
